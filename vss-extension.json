{
    "manifestVersion": 1,
    "id": "akeylessExtensionsAzdo",
    "name": "AKeyless Extensions for Azure DevOps",
    "version": "0.1.0",
    "public": false,
    "publisher": "lancelotsoftware",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "An Azure DevOps extension to safely retrieve and use secrets from your AKeyless vault.",
    "categories": [
        "Azure Pipelines",
        "Build and release"
    ],
    "tags": [
        "release",
        "build",
        "akeyless",
        "secrets",
        "variable",
        "password",
        "token"
    ],
    "links": {
        "support": {
            "uri": "https://github.com/LanceMcCarthy/akeyless-extension-azdo/issues"
        }
    },
    "icons": {
        "default": "images/akeyless-unlock-icon.png"
    },
    "branding": {
        "color": "#28a745",
        "theme": "dark"
    },
    "screenshots": [
        {
            "path": "images/screenshot.png"
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/LanceMcCarthy/akeyless-extension-azdo"
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },
    "files": [
        {
            "path": "tasks"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "akeyless-extensions-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks"
            }
        }
    ]
}